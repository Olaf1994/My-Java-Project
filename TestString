/**
*the diffrence between "==" and "equals"
*"==" means compare the address in the memory of objects
*"equals" means compare the content of objects ;
*/
public class Test10
{
	public static void main(String[] args){
		//String a = "hello" ;
		//String b = "hello" ;
		//System.out.println(a==b) ;          //true
		//System.out.println(a.equals(b)) ;  //true

		//String a = new String("hello" );
		//String b = "hello" ;
		//System.out.println(a==b) ;          //false
		//System.out.println(a.equals(b)) ;  //true

		String a = new String("hello" ) ;
		String b = new String("hello" ) ;
		System.out.println(a==b) ;          //false
		System.out.println(a.equals(b)) ;  //true
	}
}
